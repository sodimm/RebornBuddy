    <CodeChunk Name="69230">
      <![CDATA[				
        var diveToLocation = new Vector3("-328.463,-50.55911,-254.9518");
        var houseLocation = new Vector3("-337.0634,-53.65659,-277.9061");

        // Dive underwater.
        while (Core.Me.Distance(diveToLocation) > 1f)
        {
          Navigator.PlayerMover.MoveTowards(diveToLocation);
          await Coroutine.Yield();
        }

        // Move into House of the First Light.
        while (Core.Me.Distance(houseLocation) > 1f)
        {
          Navigator.PlayerMover.MoveTowards(houseLocation);
          await Coroutine.Yield();
        }

        // Get object.
        var relicsDictionary = new Dictionary<uint,Vector3>()
        {
          {2010865,new Vector3("-335.5917,-53.75763,-277.9126")},
          {2010866,new Vector3("-335.5917,-54.1543,-293.4158")},
          {2010867,new Vector3("-351.6442,-53.6355,-295.7352")}
        };

        var activeId = 0u;
        foreach (var id in relicsDictionary.Keys)
        {
          var obj = GameObjectManager.GetObjectByNPCId(id);
          if (obj != null && obj.IsTargetable)
          {
            activeId = id;
            break;
          }
        }

        // Let Flightor do the rest.
        while (Core.Me.Distance(relicsDictionary[activeId]) > 5f)
        {
          var flightParams = new ff14bot.Pathing.FlyToParameters(relicsDictionary[activeId]) {  CheckIndoors = false  };
          ff14bot.Pathing.Flightor.MoveTo(flightParams);
          await Coroutine.Yield();
        }

        Navigator.PlayerMover.MoveStop();
        
        if (GameObjectManager.GetObjectByNPCId(activeId).IsVisible)  {  GameObjectManager.GetObjectByNPCId(activeId).Interact();  }

        while (ConditionParser.GetQuestStep(69230) != 255)
        {
          if (Talk.DialogOpen)  {  Talk.Next();  }
          await Coroutine.Yield();
        }

        // If position 2, exit thru door or Flightor gets stuck.
        if (activeId == 2010866)
        {
          var exitLoc = new Vector3("-332.3985,-53.90364,-260.3446");
          
          while (Core.Me.Distance(exitLoc) > 1f)
          {
            Navigator.PlayerMover.MoveTowards(exitLoc);
            await Coroutine.Yield();
          }
        }
        
        Navigator.PlayerMover.MoveStop();
      ]]>
    </CodeChunk>
